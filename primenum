#include<stdio.h>
int primenum()
{
	int i,n,m=0,flag=0;
	printf("Enter any number to check if it's prime or not'");
	scanf("%d",&n);
	m=n/2;
	for(i=2;i<m;++i)
	{
		if(n%i==0)
		{
		  printf("It's not a prime number");
		  flag=1;
		  break;
	    }
	}
	if(flag==0)
	{
		printf("It's a prime number");
	}
	
}
int main()
{
	primenum();
	return 0;
}
